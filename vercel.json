{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://admin.shopify.com https://*.myshopify.com"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "22"
    }
  },
  "rootDirectory": "."
}
